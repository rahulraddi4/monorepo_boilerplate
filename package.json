{
  "name": "monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/web",
    "packages/*"
  ],
  "scripts": {
    "web": "cd apps/web && npm run dev",
    "mobile": "cd apps/mobile && npm run start",
    "ios": "cd apps/mobile && npm run ios",
    "android": "cd apps/mobile && yarn android"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
